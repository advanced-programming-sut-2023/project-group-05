<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.view.GameRoomMenu"
            styleClass="background"
            stylesheets="@../css/Menu/GameRoomMenu.css"
            prefHeight="400.0" prefWidth="600.0">

    <center>
        <VBox fx:id="userList">
            <Text text = "You have joined game room, wait for server admin to start when everyone joined" />
            <HBox>
                <Button fx:id = "leaveButton" text = "LEAVE" />
                <Button fx:id = "spectatorButton" text = "SPECTATOR" />
            </HBox>
        </VBox>
    </center>

</BorderPane>
